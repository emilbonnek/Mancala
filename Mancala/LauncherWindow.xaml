<Window x:Class="Mancala.LauncherWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mancala"
        mc:Ignorable="d"
        Title="LauncherWindow" Height="450" Width="600">
    <Grid>
        <Button Content="Start new game" HorizontalAlignment="Left" Click="Button_Click" Margin="261,391,0,0" VerticalAlignment="Top" Width="75"/>
        
        <RadioButton x:Name="_RadioPlayer" Content="Player" Checked="_RadioPlayer_Checked" HorizontalAlignment="Left" Margin="95,23,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="_RadioAI" Content="AI" Checked="_RadioPlayer_Checked" HorizontalAlignment="Left" Margin="95,59,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.546,0.474" />
        <TextBox x:Name="_Name1" HorizontalAlignment="Left" Height="23" Margin="26,320,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="_Name2" HorizontalAlignment="Left" Height="23" Margin="406,320,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
